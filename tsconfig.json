{
  "compilerOptions": {                          
    "lib": ["DOM", "DOM.Iterable", "ESNext"],                                       
    "jsx": "react-jsx",                                
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,                      
     "allowJs": true,                              
    "noEmit": true,                               
    "allowSyntheticDefaultImports": true,           
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                     
    "skipLibCheck": true                               
  },
  "include": ["src"]
}
